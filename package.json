{
  "name": "angular2-knob",
  "version": "1.1.12",
  "description": "Angular directive for Knob component powered by d3.js v4 without jQuery dependencies",
  "main": "dist/angular2-knob.umd.js",
  "scripts": {
    "copy": "node tools/copy-package.js && node tools/cleanup.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle && npm run copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xzegga/angular-knob"
  },
  "keywords": [
    "Knob",
    "Angular",
    "d3v4",
    "angular-knob"
  ],
  "author": "Raul Escamilla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xzegga/angular-knob/issues"
  },
  "homepage": "https://github.com/xzegga/angular-knob#readme",
  "dependencies": {
    "@angular/cli": "^6.0.3",
    "@types/d3": "^4.10.0",
    "@types/d3-selection": "^1.1.0",
    "@types/d3-selection-multi": "^1.0.6",
    "d3": "^4.13.0",
    "d3-selection-multi": "^1.0.1"
  },
  "devDependencies": {
    "@angular/compiler": "^4.3.6",
    "@angular/compiler-cli": "^4.3.6",
    "@angular/core": "^4.4.7",
    "@types/d3": "^4.13.0",
    "@types/d3-selection": "^1.3.0",
    "@types/d3-selection-multi": "^1.0.6",
    "d3-selection-multi": "^1.0.1",
    "del": "^3.0.0",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.49.2",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "^5.5.11",
    "typescript": "^2.8.3",
    "uglify-js": "^3.0.28",
    "zone.js": "^0.8.17"
  }
}
